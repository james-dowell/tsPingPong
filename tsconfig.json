{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "jsx": "react",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": false,
        "noImplicitReturns": false,
        "sourceMap": true,
        "allowUnreachableCode": true
    },
    "compileOnSave": false,
    "filesGlob": [
        "./src/**/*.ts",
        "./src/**/*.tsx",
        "./typings/index.d.ts"
    ]
}